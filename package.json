{
  "name": "oasis-ajna-rewards-redeemer",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT",
  "scripts": {
    "start:local": "npx hardhat node --max-memory 4192 --fork https://eth-mainnet.alchemyapi.io/v2/XfCCdJqU6_eHH8GUL2FdxdOP6kplx40W",
    "deploy:test": "npx hardhat run --network localhost scripts/snapshot/seed-contract-and-db-hardhat.ts",
    "test": "npx hardhat test",
    "test:ts": "mocha -r ts-node/register 'scripts/**/*test.ts'",
    "build": "npx hardhat compile",
    "clean": "rm -rf artifacts cache typechain",
    "lint:fix": "./node_modules/.bin/solhint --fix -f table contracts/*.sol contracts/**/*.sol && ./node_modules/.bin/eslint --fix test/**/*.ts scripts/**/*.ts",
    "lint:contracts": "./node_modules/.bin/solhint -f table contracts/*.sol contracts/**/*.sol",
    "lint:typescript": "./node_modules/.bin/eslint test/**/*.ts scripts/**/*.ts",
    "lint": "yarn lint:contracts && yarn lint:typescript",
    "format:fix": "./node_modules/.bin/prettier --write contracts/*.sol contracts/**/*.sol test/**/*.ts scripts/**/*.ts",
    "format": "./node_modules/.bin/prettier --check contracts/*.sol contracts/**/*.sol test/**/*.ts"
  },
  "devDependencies": {
